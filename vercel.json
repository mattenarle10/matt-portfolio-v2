{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.mattenarle.com"
        }
      ],
      "destination": "https://mattenarle.com/:path*",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "mattenarle.vercel.app"
        }
      ],
      "destination": "https://mattenarle.com/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ]
}
